.container
    display block
    width 100%
    max-width 1440px
    margin-left auto
    margin-right auto
    @media (max-width: 1500px)
        padding-left 30px
        padding-right 30px
    @media (max-width: 1024px)
        max-width 960px
        padding-left 0
        padding-right 0
    @media (max-width: 990px)
        padding-left 15px
        padding-right 15px
        max-width none
    @media (max-width: 767px)
        padding-left 10px
        padding-right 10px

